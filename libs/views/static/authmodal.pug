.modal.log_sign
	.overlay
		a.close(data-modal)
			i.ion-ios-close-empty
		.box#in
			.box-body
				h5 Welcome back!
				p Sign in below, you don't need to have an account from before.
			.box-footer
				.buttons
					a.button.twitter.block(href="/auth/login/twitter") #[i.ion-social-twitter] Sign in with Twitter
					a.button.block(href="/auth/login/github") #[i.ion-social-github] Sign in with Github
