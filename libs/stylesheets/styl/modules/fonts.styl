@font-face
  font-family 'Fakt'
  src url('../fonts/FaktPro-Normal.eot')
  src url('../fonts/FaktPro-Normal.eot?#iefix') format('embedded-opentype'),
       url('../fonts/FaktPro-Normal.woff') format('woff'),
       url('../fonts/FaktPro-Normal.ttf') format('truetype')
  font-weight 400
  font-style normal

